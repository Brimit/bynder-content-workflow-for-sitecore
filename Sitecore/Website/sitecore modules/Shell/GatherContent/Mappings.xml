    <FormPage Header="Template mappings">
      <CodeBeside Type="GatherContent.Connector.Website.Shell.Mappings, GatherContent.Connector.Website" />


      <Listview ID="ProjectTemplateMappings" View="Details" Width="100%" Background="white">
        <ListviewHeader>
          <ListviewHeaderItem Name="name" Header="GC Project" />
          <ListviewHeaderItem Name="path" Header="GC Template" />
          <ListviewHeaderItem Name="language" Header="Sc Template" />
          <ListviewHeaderItem Name="version" Header="Last Mapped" />
          <ListviewHeaderItem Name="lastUpdatedGcDate" Header="Last updated in GC" />
